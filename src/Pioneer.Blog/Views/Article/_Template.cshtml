@{
    Layout = "~/Views/Shared/_LayoutPublic.cshtml";
}
@model Pioneer.Blog.Model.Views.ArticleViewModel
@section metaPage
{
  <meta property="og:type" content="article">
  <meta property="og:image" content="@(AppConfiguration.Value.SiteUrl + Model.Image)">
  <meta property="article:author" content="https://www.facebook.com/ChadMRamos/">
  <meta property="article:section" content="@Model.ArticleSection">
  <meta property="article:published_time" content="@Model.ArticlePublishedTime">
  <meta property="article:modified_time" content="@Model.ArticleModifiedTime">
  <meta property="og:updated_time" content="@Model.ArticleModifiedTime">
  <meta itemprop="image" content="@(AppConfiguration.Value.SiteUrl + Model.Image)">
}

@RenderBody()
